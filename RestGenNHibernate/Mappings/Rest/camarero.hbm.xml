<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="RestGenNHibernate"
		                   namespace="RestGenNHibernate.EN.Rest">
		    
		    
			
				<class name="CamareroEN" table="Camarero"> 
				  	
						<id name="Id" column="idCamarero" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
			
		  			
						<bag name="Rol" >
							<key>
							
								
								<column name="FK_idCamarero_idCamarero"/>
					      	</key>
							<one-to-many class="RestGenNHibernate.EN.Rest.RolEN"/>
					    </bag>
					
			
		  			
						<bag name="Pedido" >
							<key>
							
								
								<column name="FK_idCamarero_idCamarero"/>
					      	</key>
							<one-to-many class="RestGenNHibernate.EN.Rest.PedidoEN"/>
					    </bag>
					
					
					
					
		
		 </class>	
		  
		  <query name="CamareroENReadAllHQL">from CamareroEN</query>
		</hibernate-mapping>
	